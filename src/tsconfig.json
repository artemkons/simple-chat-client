{
  "compilerOptions": {
    "target": "es2016",
    "jsx": "react",
    "module": "ES2022",
    "moduleResolution":"Bundler",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
